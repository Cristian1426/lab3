
<h1>Names of all the monsters</h1>

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Nombre</th>
        </tr>
    </thead>

    <tbody>
        <% @monsters.each do |monster| %>
            <tr>

                <td><%= link_to monster.name, monster %></td>
                
            </tr>
        <% end %>
    </tbody>
</table>